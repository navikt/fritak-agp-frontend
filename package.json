{
  "name": "fritak-agp-frontend",
  "version": "0.1.131",
  "private": true,
  "homepage": "/fritak-agp",
  "scripts": {
    "start:js": "react-scripts start --development",
    "build:js": "react-scripts build",
    "start:css": "less-watch-compiler --config less.json",
    "build:css": "less-watch-compiler --run-once --config less.json",
    "dev": "npm-run-all -p start:*",
    "build": "npm run build:css && npm run build:js",
    "test": "react-scripts test --color --watchAll=false --coverage --env=jest-environment-jsdom-sixteen",
    "lint": "eslint src/**/*.ts*",
    "lint:fix": "eslint src/**/*.ts* --fix",
    "pretty-please": "pretty-quick --staged"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "ie 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "ie 11"
    ]
  },
  "dependencies": {
