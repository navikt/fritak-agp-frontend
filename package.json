{
  "name": "fritak-agp-frontend",
  "version": "1.0.26",
  "private": true,
  "homepage": "/fritak-agp",
  "proxy": "http://localhost:8080",
  "type": "module",
  "scripts": {
    "dev": "vite  --host",
    "start": "vite",
    "start:host": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run --coverage",
    "lint": "eslint src/**/*.ts*",
    "lint:fix": "eslint src/**/*.ts* --fix",
    "prepare": "husky",
    "postinstall": "cross-os postinstall",
    "test:int": "playwright test",
    "test:watch": "vitest",
    "knip": "knip --no-exit-code --reporter json"
  },
  "cross-os": {
    "postinstall": {
      "darwin": "mkdir -p ./node_modules && touch ./node_modules/.metadata_never_index"
    }
  },
  "engines": {
    "node": ">=22",
    "yarn": ">=1.22"
  },
  "engineStrict": true,
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@grafana/faro-web-sdk": "^2.1.0",
    "@grafana/faro-web-tracing": "^2.1.0",
    "@navikt/aksel-icons": "^7.39.1",
    "@navikt/ds-css": "^7.39.1",
    "@navikt/ds-react": "^7.39.1",
    "@navikt/fnrvalidator": "^2.2.1",
    "@navikt/nav-dekoratoren-moduler": "^3.6.1",
    "@navikt/textparser": "^2.4.0",
    "@navikt/virksomhetsvelger": "^1.0.6",
    "csp-header": "^6.3.0",
    "date-fns": "^4.1.0",
    "html-react-parser": "^5.2.11",
    "i18next": "^25.7.4",
    "jsdom": "^27.4.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-helmet-async": "^2.0.5",
    "react-i18next": "^16.5.3",
    "react-router-dom": "^7.12.0",
    "uuid": "13.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.57.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "30.x",
    "@types/jest-axe": "3.x",
    "@types/node": "24.x",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/eslint-plugin": "^1.6.6",
    "@vitest/ui": "^4.0.18",
    "cross-os": "1.5.0",
    "eslint": "^9.39.2",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "husky": "9.x",
    "jest-axe": "10.x",
    "knip": "^5.82.1",
    "lint-staged": "^16.2.7",
    "mockdate": "^3.0.5",
    "msw": "^2.12.7",
    "prettier": "3.x",
    "rollup-plugin-visualizer": "^6.0.5",
    "sass-embedded": "^1.97.2",
    "timezone-mock": "1.x",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.1",
    "vite": "^7",
    "vite-plugin-compression": "^0.5.1",
    "vite-tsconfig-paths": "^6.0.4",
    "vitest": "^4.0.18",
    "vitest-sonar-reporter": "^3.0.0"
  },
  "packageManager": "yarn@4.12.0"
}
