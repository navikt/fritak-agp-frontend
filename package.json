{
  "name": "fritak-agp-frontend",
  "version": "0.5.8",
  "private": true,
  "homepage": "/fritak-agp",
  "proxy": "http://localhost:8080",
  "scripts": {
    "start": "vite",
    "start:host": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run --coverage",
    "lint": "eslint src/**/*.ts*",
    "lint:fix": "eslint src/**/*.ts* --fix",
    "prepare": "husky install",
    "postinstall": "cross-os postinstall",
    "test:int": "testcafe firefox --hostname localhost tests/ --skip-js-errors --dev --assertion-timeout 300",
    "test:watch": "vitest"
  },
  "cross-os": {
    "postinstall": {
      "darwin": "mkdir -p ./node_modules && touch ./node_modules/.metadata_never_index"
    }
  },
  "engines": {
    "node": ">=14",
    "yarn": ">=1.22"
  },
  "engineStrict": true,
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "ie 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "ie 11"
    ]
  },
  "dependencies": {
    "@navikt/aksel-icons": "^5.9.2",
    "@navikt/bedriftsmeny": "^6.12.0",
    "@navikt/ds-css": "^5.9.2",
    "@navikt/ds-icons": "^3.4.3",
    "@navikt/ds-react": "^5.9.2",
    "@navikt/fnrvalidator": "^1.3.3",
    "@navikt/nav-dekoratoren-moduler": "^2.1.4",
    "@navikt/textparser": "^2.2.0",
    "@sentry/react": "^7.90.0",
    "date-fns": "^2.30.0",
    "dayjs": "1.11.10",
    "i18next": "^23.7.3",
    "jsdom": "^23.0.1",
    "react": "^18.2.0",
    "react-app-polyfill": "3.0.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.5.0",
    "react-router-dom": "^6.21.0",
    "typescript": "^5.3.3",
    "uuid": "9.0.1"
  },
  "snyk": true,
  "devDependencies": {
    "@testing-library/dom": "^9.3.3",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/testcafe": "^5.0.1",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "29.x",
    "@types/jest-axe": "3.x",
    "@types/node": "20.x",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.17",
    "@types/react-router-dom": "5.x",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.13.1",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.0.2",
    "cross-os": "1.5.0",
    "eslint": "^8.54.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-testcafe": "^0.2.1",
    "eslint-plugin-typescript": "^0.14.0",
    "eslint-plugin-vitest": "^0.3.10",
    "husky": "8.x",
    "jest-axe": "8.x",
    "less": "^4.2.0",
    "lint-staged": "^15.1.0",
    "mockdate": "^3.0.5",
    "msw": "^2.0.10",
    "prettier": "3.x",
    "sass": "1.x",
    "snyk": "1.x",
    "testcafe": "^3.4.0",
    "testcafe-react-selectors": "^5.0.3",
    "timezone-mock": "1.x",
    "vite": "^5.0.6",
    "vite-tsconfig-paths": "^4.2.2",
    "vitest": "^1.0.2",
    "vitest-sonar-reporter": "^0.5.0"
  }
}
