{
  "name": "fritak-agp-frontend",
  "version": "0.5.15",
  "private": true,
  "homepage": "/fritak-agp",
  "proxy": "http://localhost:8080",
  "scripts": {
    "start": "vite",
    "start:host": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run --coverage",
    "lint": "eslint src/**/*.ts*",
    "lint:fix": "eslint src/**/*.ts* --fix",
    "prepare": "husky",
    "postinstall": "cross-os postinstall",
    "test:int": "playwright test",
    "test:watch": "vitest"
  },
  "cross-os": {
    "postinstall": {
      "darwin": "mkdir -p ./node_modules && touch ./node_modules/.metadata_never_index"
    }
  },
  "engines": {
    "node": ">=14",
    "yarn": ">=1.22"
  },
  "engineStrict": true,
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "ie 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "ie 11"
    ]
  },
  "dependencies": {
    "@navikt/aksel-icons": "^7.0.1",
    "@navikt/bedriftsmeny": "^6.13.2-rc2",
    "@navikt/ds-css": "^7.0.1",
    "@navikt/ds-icons": "^3.4.3",
    "@navikt/ds-react": "^7.0.1",
    "@navikt/fnrvalidator": "^2.1.2",
    "@navikt/nav-dekoratoren-moduler": "^3.1.0",
    "@navikt/textparser": "^2.4.0",
    "@sentry/react": "^8.31.0",
    "date-fns": "^3.6.0",
    "dayjs": "1.11.13",
    "i18next": "^23.15.1",
    "jsdom": "^25.0.0",
    "react": "^18.2.0",
    "react-app-polyfill": "3.0.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^15.0.1",
    "react-router-dom": "^6.26.1",
    "typescript": "^5.6.2",
    "uuid": "10.0.0"
  },
  "snyk": true,
  "devDependencies": {
    "@playwright/test": "^1.45.3",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "29.x",
    "@types/jest-axe": "3.x",
    "@types/node": "22.x",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/react-router-dom": "5.x",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^2.0.5",
    "@vitest/ui": "^2.0.5",
    "cross-os": "1.5.0",
    "eslint": "^9.10.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-react": "^7.35.2",
    "eslint-plugin-typescript": "^0.14.0",
    "eslint-plugin-vitest": "^0.5.3",
    "husky": "9.x",
    "jest-axe": "9.x",
    "less": "^4.2.0",
    "lint-staged": "^15.2.10",
    "mockdate": "^3.0.5",
    "msw": "^2.4.9",
    "prettier": "3.x",
    "sass": "1.x",
    "snyk": "1.x",
    "timezone-mock": "1.x",
    "typescript-eslint": "^8.4.0",
    "vite": "^5.4.8",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.0.5",
    "vitest-sonar-reporter": "^2.0.0"
  }
}
