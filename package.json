{
  "name": "fritak-agp-frontend",
  "version": "0.5.3",
  "private": true,
  "homepage": "/fritak-agp",
  "proxy": "http://localhost:8080",
  "scripts": {
    "start": "GENERATE_SOURCEMAP=false BROWSER=firefox react-scripts start --development",
    "serve": "react-scripts start --development",
    "build": "react-scripts build",
    "test": "react-scripts test --color --watchAll=false --coverage",
    "lint": "eslint src/**/*.ts*",
    "lint:fix": "eslint src/**/*.ts* --fix",
    "pretty-please": "pretty-quick --staged",
    "pretty-commit": "pretty-quick --staged --pattern \"**/*.*(ts|tsx|js)\"",
    "prepare": "husky install",
    "postinstall": "cross-os postinstall",
    "test:int": "testcafe firefox --hostname localhost tests/ --skip-js-errors --dev --assertion-timeout 300",
    "test:watch": "react-scripts test --color"
  },
  "cross-os": {
    "postinstall": {
      "darwin": "mkdir -p ./node_modules && touch ./node_modules/.metadata_never_index"
    }
  },
  "engines": {
    "node": ">=14",
    "yarn": ">=1.22"
  },
  "engineStrict": true,
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "ie 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "ie 11"
    ]
  },
  "dependencies": {
    "@navikt/aksel-icons": "^3.2.1",
    "@navikt/bedriftsmeny": "^4.0.2",
    "@navikt/ds-css": "^3.2.1",
    "@navikt/ds-icons": "^3.2.1",
    "@navikt/ds-react": "^3.2.1",
    "@navikt/fnrvalidator": "^1.3.3",
    "@navikt/nav-dekoratoren-moduler": "^1.6.5",
    "@sentry/react": "^7.53.0",
    "classnames": "^2.3.2",
    "dayjs": "1.11.7",
    "i18next": "^22.5.0",
    "nav-frontend-core": "^6.0.1",
    "nav-frontend-grid": "^2.0.2",
    "nav-frontend-grid-style": "1.0.3",
    "nav-frontend-js-utils": "^1.0.20",
    "nav-frontend-lenker": "^2.0.2",
    "nav-frontend-lenker-style": "^2.0.2",
    "nav-frontend-skjema": "^4.0.6",
    "nav-frontend-skjema-style": "^3.0.3",
    "nav-frontend-tekstomrade": "^3.0.2",
    "nav-frontend-typografi": "^4.0.2",
    "nav-frontend-typografi-style": "^2.0.2",
    "prop-types": "^15.8.0",
    "react": "^18.2.0",
    "react-app-polyfill": "3.0.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.3.1",
    "react-router-dom": "^6.11.2",
    "typescript": "^5.0.3",
    "uuid": "9.0.0"
  },
  "snyk": true,
  "devDependencies": {
    "@smartive/testcafe-utils": "^1.3.1",
    "@testing-library/dom": "^9.3.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "29.x",
    "@types/jest-axe": "3.x",
    "@types/node": "20.x",
    "@types/react": "^18.2.7",
    "@types/react-collapse": "^5.0.1",
    "@types/react-dom": "^18.2.4",
    "@types/react-modal": "3.x",
    "@types/react-router-dom": "5.x",
    "@types/uuid": "^9.0.0",
    "cross-os": "1.5.0",
    "eslint-plugin-testcafe": "^0.2.1",
    "eslint-plugin-typescript": "^0.14.0",
    "husky": "8.x",
    "jest-axe": "7.x",
    "mockdate": "^3.0.5",
    "prettier": "2.x",
    "pretty-quick": "3.x",
    "react-scripts": "^5.0.0",
    "react-test-renderer": "18.x",
    "sass": "1.x",
    "snyk": "1.x",
    "testcafe": "^2.6.1",
    "testcafe-react-selectors": "^5.0.3",
    "timezone-mock": "1.x",
    "ts-jest": "^29.1.0",
    "yet-another-fetch-mock": "4.1.0"
  }
}
