{
  "name": "fritak-agp-frontend",
  "version": "0.5.3",
  "private": true,
  "homepage": "/fritak-agp",
  "proxy": "http://localhost:8080",
  "scripts": {
    "start": "vite",
    "start:host": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run --coverage",
    "lint": "eslint src/**/*.ts*",
    "lint:fix": "eslint src/**/*.ts* --fix",
    "pretty-please": "pretty-quick --staged",
    "pretty-commit": "pretty-quick --staged --pattern \"**/*.*(ts|tsx|js)\"",
    "prepare": "husky install",
    "postinstall": "cross-os postinstall",
    "test:int": "testcafe firefox --hostname localhost tests/ --skip-js-errors --dev --assertion-timeout 300",
    "test:watch": "react-scripts test --color"
  },
  "cross-os": {
    "postinstall": {
      "darwin": "mkdir -p ./node_modules && touch ./node_modules/.metadata_never_index"
    }
  },
  "engines": {
    "node": ">=14",
    "yarn": ">=1.22"
  },
  "engineStrict": true,
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "ie 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "ie 11"
    ]
  },
  "dependencies": {
    "@navikt/aksel-icons": "^3.2.1",
    "@navikt/bedriftsmeny": "^6.12.0",
    "@navikt/ds-css": "^3.2.1",
    "@navikt/ds-icons": "^3.2.1",
    "@navikt/ds-react": "^3.2.1",
    "@navikt/fnrvalidator": "^1.3.3",
    "@navikt/nav-dekoratoren-moduler": "^1.6.5",
    "@navikt/textparser": "^2.2.0",
    "@sentry/react": "^7.54.0",
    "classnames": "^2.3.2",
    "dayjs": "1.11.8",
    "i18next": "^23.4.9",
    "prop-types": "^15.8.0",
    "react": "^18.2.0",
    "react-app-polyfill": "3.0.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.3.1",
    "react-router-dom": "^6.18.0",
    "typescript": "^5.2.2",
    "uuid": "9.0.0"
  },
  "snyk": true,
  "devDependencies": {
    "@smartive/testcafe-utils": "^1.3.3",
    "@testing-library/dom": "^9.3.3",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/testcafe": "^5.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "29.x",
    "@types/jest-axe": "3.x",
    "@types/node": "20.x",
    "@types/react": "^18.2.34",
    "@types/react-collapse": "^5.0.1",
    "@types/react-dom": "^18.2.4",
    "@types/react-modal": "3.x",
    "@types/react-router-dom": "5.x",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "@vitejs/plugin-react": "^4.1.0",
    "@vitest/coverage-v8": "^0.34.6",
    "cross-os": "1.5.0",
    "eslint": "^8.52.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-testcafe": "^0.2.1",
    "eslint-plugin-typescript": "^0.14.0",
    "eslint-plugin-vitest": "^0.3.8",
    "husky": "8.x",
    "jest-axe": "7.x",
    "less": "^4.2.0",
    "mockdate": "^3.0.5",
    "prettier": "2.x",
    "pretty-quick": "3.x",
    "react-test-renderer": "18.x",
    "sass": "1.x",
    "snyk": "1.x",
    "testcafe": "^3.3.0",
    "testcafe-react-selectors": "^5.0.3",
    "timezone-mock": "1.x",
    "vite": "^4.5.0",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.6",
    "vitest-sonar-reporter": "^0.5.0",
    "yet-another-fetch-mock": "4.1.0"
  }
}
