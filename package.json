{
  "name": "fritak-agp-frontend",
  "version": "1.0.16",
  "private": true,
  "homepage": "/fritak-agp",
  "proxy": "http://localhost:8080",
  "type": "module",
  "scripts": {
    "dev": "vite  --host",
    "start": "vite",
    "start:host": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run --coverage",
    "lint": "eslint src/**/*.ts*",
    "lint:fix": "eslint src/**/*.ts* --fix",
    "prepare": "husky",
    "postinstall": "cross-os postinstall",
    "test:int": "playwright test",
    "test:watch": "vitest",
    "knip": "knip --no-exit-code --reporter json"
  },
  "cross-os": {
    "postinstall": {
      "darwin": "mkdir -p ./node_modules && touch ./node_modules/.metadata_never_index"
    }
  },
  "engines": {
    "node": ">=22",
    "yarn": ">=1.22"
  },
  "engineStrict": true,
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@navikt/aksel-icons": "^7.23.1",
    "@navikt/ds-css": "^7.23.1",
    "@navikt/ds-react": "^7.23.1",
    "@navikt/fnrvalidator": "^2.1.2",
    "@navikt/nav-dekoratoren-moduler": "^3.2.3",
    "@navikt/textparser": "^2.4.0",
    "@navikt/virksomhetsvelger": "^1.0.3",
    "date-fns": "^4.1.0",
    "dayjs": "1.11.13",
    "i18next": "^25.2.1",
    "jsdom": "^26.1.0",
    "react": "^19.1.0",
    "react-app-polyfill": "3.0.0",
    "react-dom": "^19.1.0",
    "react-i18next": "^15.5.3",
    "react-router-dom": "^7.6.2",
    "typescript": "^5.8.3",
    "uuid": "11.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@grafana/faro-web-sdk": "^1.18.2",
    "@grafana/faro-web-tracing": "^1.18.2",
    "@playwright/test": "^1.53.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "30.x",
    "@types/jest-axe": "3.x",
    "@types/node": "24.x",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "@vitejs/plugin-react": "^4.5.2",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "cross-os": "1.5.0",
    "eslint": "^9.29.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-vitest": "^0.5.3",
    "husky": "9.x",
    "jest-axe": "10.x",
    "knip": "^5.61.2",
    "lint-staged": "^16.1.2",
    "mockdate": "^3.0.5",
    "msw": "^2.10.2",
    "prettier": "3.x",
    "rollup-plugin-visualizer": "^6.0.3",
    "sass-embedded": "^1.89.2",
    "timezone-mock": "1.x",
    "typescript-eslint": "^8.34.1",
    "vite": "^6",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^3.2.4",
    "vitest-sonar-reporter": "^2.0.1"
  }
}
